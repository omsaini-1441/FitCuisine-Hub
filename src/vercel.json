{
    "builds":[
        {
        "src":"./app.js",
        "use":"@vercel/node"
        },
        {
            "src": "./front/build",
            "use": "@vercel/static"
          }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"app.js"
        }
    ],
    "rewrites": [
        { "source" : "/(.*)", "destination" : "/"}
    ]
}